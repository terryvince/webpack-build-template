{
  "name": "webpack-build-template",
  "version": "1.0.0",
  "description": "webpack build template",
  "main": "main.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "set NODE_ENV=development&&webpack-dev-server --inline --config build/webpack.dev.config.js",
    "build": "set NODE_ENV=production&&webpack --config build/webpack.pro.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron_dev": "npm run build && electron .",
    "electron_build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.terryvince.www",
    "copyright": "terryvince",
    "productName": "HelloWorld",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "./install",
      "app": "./"
    }
  },
  "author": "terryvince",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.10",
    "ejs": "^2.6.1",
    "ejs-html-loader": "^3.1.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.40.2",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "fast-sass-loader": "^1.4.7",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-script-injection-webpack-plugin": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-reload-plugin": "^2.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "jquery": "^3.4.1",
    "vue": "^2.6.7"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "iOS >= 6",
    "Android > 4.1",
    "not ie <= 8",
    "Firefox > 20"
  ]
}
